
(rules PCB minikeyboard
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (eu.mihosoft.freerouting.autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 736)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.7)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.6)
    )
  )
  (rule
    (width 250.0)
    (clear 200.2)
    (clear 125.0 (type smd_to_turn_gap))
    (clear 50.0 (type smd_smd))
  )
  (padstack "Via[0-1]_800:400_um"
    (shape
      (circle F.Cu 800.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 800.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_800:400_um" "Via[0-1]_800:400_um" default
  )
  (via 
    "Via[0-1]_800:400_um-kicad_default" "Via[0-1]_800:400_um" "kicad_default"
  )
  (via_rule
    default "Via[0-1]_800:400_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_800:400_um-kicad_default"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    SCK "Net-(A1-Pad15)" "Net-(A1-Pad30)" MOSI GND SS "Net-(A1-Pad28)" "Net-(A1-Pad12)"
    +5V GATE "CODE_OUT_D" "Net-(A1-Pad10)" "CODE_OUT_C" "KEY_IN_D" "CODE_OUT_B" "KEY_IN_C"
    "CODE_OUT_A" "KEY_IN_B" "Net-(A1-Pad22)" "KEY_IN_A" "Net-(A1-Pad21)" "KEY_OUT_C" "Net-(A1-Pad20)" "Net-(A1-Pad19)"
    "Net-(A1-Pad3)" "Net-(A1-Pad18)" "KEY_OUT_A" "Net-(A1-Pad17)" "KEY_OUT_B" "Net-(D1-Pad1)" "Net-(D2-Pad1)" "Net-(D3-Pad1)"
    "Net-(J1-PadT)"
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)